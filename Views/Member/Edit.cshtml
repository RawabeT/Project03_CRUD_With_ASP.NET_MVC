@model Member
@{
    var member = (Member)ViewData["members"];
}

<form asp-action="Edit" method="post" class="form-group">
    <label asp-for="Name">Memebr Name: </label>
    <div class="form-group">
        <input asp-for="Name" value="@member.Name" />
    </div>
    <label asp-for="Address">Address: </label>
    <div class="form-group">
        <input asp-for="Address" value="@member.Address" />
    </div>
    <label asp-for="ExpiryDate">Membership Expiry Date: </label>
    <div class="form-group">
        <input asp-for="ExpiryDate" value="@member.ExpiryDate" />
    </div>
    <div class="form-group">
        <input type="submit" class="btn btn-primary" />
    </div>
</form>
